{
  "name": "fastify-playground",
  "version": "0.0.1",
  "description": "",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tap test/**/*.test.js",
    "dev": "nodemon ./src --ext ts --exec 'ts-node -r tsconfig-paths/register ./src/index.ts'",
    "compile-schemas": "json2ts -i src/schemas -o src/types",
    "format": "prettier --write */**/.{js,ts}"
  },
  "keywords": [],
  "author": "bobstrange <bob.1983.g@gmail.com> (https://github.com/bobstrange)",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^8.2.0",
    "fastify": "^3.0.0",
    "fastify-autoload": "^3.3.1",
    "fastify-cli": "^2.8.0",
    "fastify-plugin": "^3.0.0",
    "fastify-sensible": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "json-schema-to-typescript": "^10.1.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "tap": "^14.11.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.2"
  }
